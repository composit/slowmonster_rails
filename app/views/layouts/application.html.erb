<!DOCTYPE html>
<html>
<head>
  <title>Slowmonster</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body ng-app='slowMonster'>
  <% if @current_user %>
    <%= link_to 'sign out', user_session_path, method: :delete %>
<% end %>
  <%= yield %>
</body>
</html>
